package dataStructures;

import java.util.Collection;
import java.util.Iterator;
import java.util.function.Function;

public interface Circleable<E> {

	public default Iterable<E> traverseClosestElements(E target, Function<E, Collection<E>> getNeighbors) {
		return new Iterable<E>() {

			@Override
			public Iterator<E> iterator() {
				return getIterator(target, getNeighbors);
			}
			
		};
	}
	
	public Iterator<E> getIterator(E target, Function<E, Collection<E>> getNeighbors);
	
	
}
