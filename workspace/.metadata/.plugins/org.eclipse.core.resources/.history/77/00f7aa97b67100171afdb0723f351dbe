package dataStructures;

import java.util.Iterator;
import java.util.function.Predicate;

public interface Circleable<E> {

	public default Iterable<E> traverseClosestElements(E target, Predicate<E> shouldTraverse) {
		return new Iterable<E>() {

			@Override
			public Iterator<E> iterator() {
				return getIterator(target, shouldTraverse);
			}
			
		};
	}
	
	public Iterator<E> getIterator(E target, Predicate<E> shouldTraverse);
	
	
}
