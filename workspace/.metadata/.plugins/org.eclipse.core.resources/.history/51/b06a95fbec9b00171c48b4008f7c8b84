package hashing;

/**
 * Some implementation that generates a sequence of random numbers
 * @author matthewslesinski
 *
 */
@FunctionalInterface
public interface RandomNumberGenerator {

	/**
	 * Generates a sequence of random, or seemingly random, numbers. This may or may not be length constrained
	 * @param length The length of the sequence
	 * @return The sequence
	 */
	public long[] generateNumbers(int length);
}
